{
    "terminal.integrated.macOptionClickForcesSelection": true,
    "workbench.editor.enablePreview": false,

    "editor.fontSize": 11,

    "editor.fontFamily": "Monaco",
    "editor.renderWhitespace": "all",

    "files.exclude": {
      "**/.git": true,
      "**/.svn": true,
      "**/.hg": true,
      "**/CVS": true,
      "**/.DS_Store": true
    },

    "editor.cursorStyle": "block",

    "editor.insertSpaces": true,

    "editor.wordWrap": "off",

    "files.associations": { "*.tx": "html" },
    "vim.useSystemClipboard":true,
    "vim.cmdLineInitialColon": true,

    "vim.hlsearch": true,
    "vim.searchHighlightColor": "rgba(255,255,255, 0.6)",

    "vim.whichwrap": "h,l,<,>,[,]",

    "vim.easymotion": true,

    "vim.visualstar": true,

    "vim.useCtrlKeys": true,
    "vim.normalModeKeyBindings": [
         {
             "before": [";"],
             "after": [":"]
         },
         {
             "before": ["ctrl+a"],
             "after": ["^"]
         }
    ],
    "vim.insertModeKeyBindings": [
         {
             "before": ["ctrl+d"],
             "after": ["Ride->debug_log();"]
         },
         {
             "before": ["j", "j"],
             "after": ["<esc>"]
         },
         {
             "before": ["ctrl+a"],
             "after": ["ctrl+o", "^"]
         }
    ],
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",

    "editor.tabSize": 2,
   
    "comments": "eslintの自動保存",
    "eslint.autoFixOnSave": true,
    "eslint.validate": [
      {
        "language": "JavaScript",
        "autoFix": true
      }
    ],
    "comments": "vueファイル内のJavaScriptに対しVeturを無効にする",
    "vetur.format.defaultFormatter.js": "none",
    "comments": "Vscodeの自動保存なし",
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "eslint.alwaysShowStatus": true,
    "breadcrumbs.enabled": true,
    "workbench.sideBar.location": "right",
    "window.zoomLevel": 1,
    "terminal.integrated.rightClickCopyPaste": true,
    "comments": "brew install reattach-to-user-namespace",
    "tabnine.experimentalAutoImports": true,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.copyOnSelection": true,
    "dart.flutterSdkPath": "/Users/hiroto.naya/Downloads/flutter",
    "[dart]": {
      "editor.formatOnSave": true,
      "editor.formatOnType": true,
      "editor.rulers": [
        80
      ],
      "editor.selectionHighlight": false,
      "editor.suggest.snippetsPreventQuickSuggestions": false,
      "editor.suggestSelection": "first",
      "editor.tabCompletion": "onlySnippets",
      "editor.wordBasedSuggestions": false
    },
    "editor.minimap.enabled": false,
    "workbench.colorCustomizations": {
        "editor.lineHighlightBackground": "#303030"
    },
    "vscode-w3cvalidation.validator-token": "Mon, 14 Sep 2020 09:25:49 GMT",
    "mssql.connections": [
      {
        "server": "{{put-server-name-here}}",
        "database": "{{put-database-name-here}}",
        "user": "{{put-username-here}}",
        "password": "{{put-password-here}}"
      }
    ],
    "workbench.activityBar.visible": false,
    "editor.inlineSuggest.enabled": true

}
